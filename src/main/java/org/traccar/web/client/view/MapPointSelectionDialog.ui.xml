<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
-->
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:gxt="urn:import:com.sencha.gxt.widget.core.client"
                xmlns:button="urn:import:com.sencha.gxt.widget.core.client.button">
    
    <ui:with field='i18n' type='org.traccar.web.client.i18n.Messages' />
    
    <gxt:Window ui:field="window" pixelSize="590, 550" modal="true" 
                headingText="{i18n.addFromMap}" focusWidget="{saveButton}">
        
        <gxt:button>
            <button:TextButton ui:field="saveButton" text="{i18n.save}" />
        </gxt:button>
        <gxt:button>
            <button:TextButton ui:field="cancelButton" text="{i18n.cancel}" />
        </gxt:button>
    
    </gxt:Window>
</ui:UiBinder>